// Online C++ compiler to run C++ program online
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
    // Write C++ code here
    vector<int>v;
    cout<<"enter n"<<endl
    int n;
    cin>>n;
    int i,j,a;
    cout<<"enter"<<endl;
    for(i=0;i<n;i++){
        cin>>a;
        v.push_back(a);
    }
    int c1=0,c2=INT_MAX,min=-1,mx=-1;
    for(i=0;i<v.size();i++){
        if(v[i]>=2)
        {
            int c=0,f=0;
        for(j=2;j<v[i];j++){
            if(v[i]%j==0){
                f=1;
                break;
            }
        }
        if(f==0){
            if(min==-1 ){
                min=v[i];
            }
            else{
                if(v[i]<min){
                    min=v[i];
                }
            }
            
            
            if(mx==-1 ){
                mx=v[i];
            }
            else{
                if(v[i]>mx){
                    mx=v[i];
                }
            }
            
        }
        }
    }
    vector<int>v1;
    v1.push_back(mx);
    sort(v.rbegin(), v.rend());
    for(i=0;i<n;i++){
        if(v[i]!=min && v[i]!=mx){
            v1.push_back(v[i]);
        }
    }
    v1.push_back(min);
    
    for(i=0;i<v1.size();i++){
        cout<<v1[i]<<" ";
    }

    return 0;
}
